{
    "name": "完本神站",
    "url": "www.wanbentxt.com",
    "version": 100,
    "search": {
        "url": "https://www.wanbentxt.com/modules/article/search.php@post->searchtype=articlename&searchkey=${key}",
		"charset": "gbk",
        "list": ".resultLeft>ul>li",
        "name": ".sortPhr>a>h3",
        "author": ".sortPhr>.author>a",
        "cover": ".sortImg>a>img",
		"summary": ".intro",
        "detail": ".sortPhr>a"
    },
    "detail": {
        "name": "h1",
        "author": ".writer>a",
        "cover": ".detailTopLeft>img",
        "summary": "tbody>tr:nth-child(3)>td:nth-child(2)",
        "status": ".detailTopLeft>span.going",
        "update": ".chapterTitle>span@match->(.+)(?<=更新至)",
        "lastChapter": ".chapterTitle>span>a",
        "catalog": ""
    },
    "catalog": {
        "list": ".chapter>ul>li",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": ".readerCon>p"
    }
}